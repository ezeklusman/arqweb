- Médicos
  - GET /medicos
  - GET /medicos/{legajo}
  - POST /medicos
    - Legajo
    - Nombre
    - Especialidad
  - PUT /medicos/{legajo}
  
- Pacientes
  - GET /pacientes
  - GET /pacientes/{dni}
  - POST /pacientes
    - Dni
    - Nombre
    - Obra Social
  - PUT /pacientes/{dni}
  
- Turnos
  - GET /turnos
  - GET /turnos/{id}
  - POST /turnos
    - ID
    - Legajo Médico
    - DNI Paciente
    - Fecha y Hora
  - PUT /turnos/{id}
  
- Especialidades
  - GET /especialidades
  - GET /especialidades/{id}
  - POST /especialidades
    - id
    - Nombre
    - Medicos (Array [{Legajos}])
  - PUT /especialidades/{id}
